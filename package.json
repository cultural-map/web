		"react-hot-toast": "^2.4.1",
		"react-i18next": "^15.0.2",
		"react-lottie": "^1.2.4",
		"react-router-dom": "^6.26.2",
		"zod": "^3.23.8",
		"zod-i18n-map": "^2.27.0",
		"zustand": "^4.5.5"
