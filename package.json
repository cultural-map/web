		"@hookform/resolvers": "^3.9.0",
		"@nextui-org/react": "^2.4.8",
		"autoprefixer": "^10.4.20",
		"classix": "^2.1.38",
		"dayjs": "^1.11.13",
		"framer-motion": "^11.5.5",
