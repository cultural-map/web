{
	"name": "cultural-map",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"npm": ">= 10.0.0",
		"node": ">= 18.17",
		"yarn": ">= 1.19.1"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"scripts": {
		"prepare": "husky",
		"build": "tsc && vite build",
		"start": "vite",
